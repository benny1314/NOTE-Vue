{
    "plugins": [
        "theme-api",
        "advanced-emoji",
        "-search",
        "search-pro",
        "github",
        "navigator",
        "ace"
    ],
    "pluginsConfig": {
        "theme-api": {
            "languages": [
                {
                    "lang": "js",
                    "name": "JavaScript",
                    "default": true
                },
                {
                    "lang": "go",
                    "name": "Go"
                }
            ]
        },
        "fontsettings": {
            "theme": "sepia",
            "family": "serif",
            "size": 1
        },
        "search-pro": {
            "cutWordLib": "nodejieba",
            "defineWord": [
                "Gitbook Use"
            ]
        },
        "github": {
            "url": "https://github.com/benny1314"
        },
        "advanced-emoji": {
            "embedEmojis": true
        }
    }
}