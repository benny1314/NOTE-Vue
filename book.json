{
    "plugins": [
        "theme-api",
        "advanced-emoji",
        "-search",
        "search-pro",
        "github",
        "ace",
        "prism",
        "-highlight",
        "toggle-chapters",
        "splitter"
    ],
    "pluginsConfig": {
        "theme-api": {
            "languages": [
                {
                    "lang": "js",
                    "name": "JavaScript",
                    "default": true
                },
                {
                    "lang": "go",
                    "name": "Go"
                }
            ]
        },
        "fontsettings": {
            "theme": "sepia",
            "family": "serif",
            "size": 1
        },
        "search-pro": {
            "cutWordLib": "nodejieba",
            "defineWord": [
                "Gitbook Use"
            ]
        },
        "github": {
            "url": "https://github.com/benny1314"
        },
        "advanced-emoji": {
            "embedEmojis": true
        },
        "prism":{
            "lang":{
                "flow":"typescripte"
            },
            "css":[
                "prismjs/themes/prism-okaidia.css"
            ]
        }
    },
    "title": "Gitbook use",
    "language": "zh-hans"
}