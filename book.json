{
    "plugins": [
        "advanced-emoji",
        "-search",
        "search-pro",
        "github",
        "prism",
        "-highlight",
        "toggle-chapters",
        "splitter"
    ],
    "pluginsConfig": {
        "fontsettings": {
            "theme": "sepia",
            "family": "serif",
            "size": 1
        },
        "search-pro": {
            "cutWordLib": "nodejieba",
            "defineWord": [
                "Gitbook Use"
            ]
        },
        "github": {
            "url": "https://github.com/benny1314"
        },
        "advanced-emoji": {
            "embedEmojis": true
        },
        "prism": {
            "lang": {
                "flow": "typescript"
            },
            "css": [
                "prismjs/themes/prism-okaidia.css"
            ]
        }
    }
}